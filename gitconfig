[apply]
	whitespace = warn
[user]
	name = Amitai Schleier
	email = schmonz-web-git@schmonz.com
[color]
	ui = true
[core]
	excludesfile = /Users/schmonz/.gitignore_global
[alias]
	graph = log --graph --all --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %Cgreen %an, %ar%Creset'
	graph2 = log --pretty=format:'%C(yellow)%h %ad%Cblue%d%Creset %s %Cgreen[%an]%Creset' --graph --all --date=short
	guygraph = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	co = checkout
	st = status
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[transfer]
	fsckobjects = true
[fetch]
	fsckobjects = true
[receive]
	fsckObjects = true
[diff]
	noprefix = true
