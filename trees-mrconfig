[ikiwiki]
checkout = 
	git clone git://git.ikiwiki.info/ ikiwiki
	cd ikiwiki
	git remote add admin ssh://b-ikiwiki@ikiwiki.branchable.com/
	git remote add github git@github.com:schmonz/ikiwiki.git

